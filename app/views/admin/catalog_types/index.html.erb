<p id="notice"><%= notice %></p>

<h1>Listing Catalog Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @catalog_types.each do |catalog_type| %>
      <tr>
        <td><%= catalog_type.name %></td>
        <td><%= link_to 'Show', catalog_type %></td>
        <td><%= link_to 'Edit', edit_catalog_type_path(catalog_type) %></td>
        <td><%= link_to 'Destroy', catalog_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Catalog type', new_catalog_type_path %>
