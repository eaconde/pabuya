<p id="notice"><%= notice %></p>

<h1>Listing Catalogs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Points</th>
      <th>catalog type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @catalogs.each do |catalog| %>
      <tr>
        <td><%= catalog.name %></td>
        <td><%= catalog.description %></td>
        <td><%= catalog.points %></td>
        <td><%= catalog.categories_id %></td>
        <td><%= link_to 'Show', catalog %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
